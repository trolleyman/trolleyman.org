
# Redirects -> https://callumgtolley.uk
http://trolleyman.org, http://www.trolleyman.org, http://callumgtolley.uk, http://www.callumgtolley.uk,
https://trolleyman.org, https://www.trolleyman.org, https://www.callumgtolley.uk {
	redir / https://callumgtolley.uk{uri}
}

https://callumgtolley.uk {
	root /opt/django
	proxy / localhost:{$DJANGO_PORT} {
		except /static
		transparent
	}
}

